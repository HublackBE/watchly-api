          "$ref": "#/components/schemas/User"
        }
      },
      "Movie": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "title": {
            "type": "string"
          },
          "slug": { "type": "string" },
          "description": { "type": "string" },
          "release_date": { "type": "string", "format": "date" },
          "director": { "type": "string" },
          "genre": { "type": "string" },
          "rating": { "type": "number" },
          "poster_path": { "type": "string" },
          "imdb_id": { "type": "string" },
          "added_by": { "type": "integer" },
          "created_at": { "type": "string", "format": "date-time" },
          "updated_at": { "type": "string", "format": "date-time" }
        }
      },
      "MovieCreate": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "slug": {
            "type": "string"
          },
          "description": { "type": "string" },
          "release_date": { "type": "string", "format": "date" },
          "director": { "type": "string" },
          "genre": { "type": "string" },
          "rating": { "type": "number" },
          "poster_path": { "type": "string" },
          "imdb_id": { "type": "string" },
          "added_by": { "type": "integer" }
        },
        "required": [
          "title"
        ],
        "description": "Provide `title`. `slug` is optional — if omitted it will be generated from the title. `added_by` is set automatically from the authenticated user when available."
      },
      "MovieUpdate": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "slug": { "type": "string" },
          "description": { "type": "string" },
          "release_date": { "type": "string", "format": "date" },
          "director": { "type": "string" },
          "genre": { "type": "string" },
