      },
      "put": {
        "tags": ["Comments"],
        "summary": "Update a comment",
        "description": "Requires authentication. Only the comment author or an admin may update this comment.",
        "security": [ { "bearerAuth": [] } ],
        "requestBody": { "required": true, "content": { "application/json": { "schema": { "$ref": "#/components/schemas/CommentUpdate" } } } },
        "responses": { "200": { "description": "Updated comment", "content": { "application/json": { "schema": { "$ref": "#/components/schemas/Comment" } } } } }
      },
      "delete": {
        "tags": ["Comments"],
        "summary": "Delete a comment",
        "description": "Requires authentication. Only the comment author or an admin may delete this comment.",
        "security": [ { "bearerAuth": [] } ],
        "responses": { "204": { "description": "Deleted" }, "404": { "description": "Not found" } }
      }
    },
