          "slug": { "type": "string" },
          "bio": { "type": "string" },
          "avatar_path": { "type": "string" },
          "date_of_birth": { "type": "string", "format": "date" }
        },
        "required": [
          "name",
          "email"
        ]
      },
      "UserUpdate": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "email": { "type": "string" },
          "password": { "type": "string" },
          "slug": { "type": "string" },
          "bio": { "type": "string" },
          "avatar_path": { "type": "string" },
          "date_of_birth": { "type": "string", "format": "date" }
